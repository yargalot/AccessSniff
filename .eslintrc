{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "semi": [2, "always"],
    "quotes": [2, "single"]
  },
  "env": {
    "es6": true,
    "node": true
  },
  "extends": "eslint:recommended"
}
